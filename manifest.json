{
    "manifest_version": 3,
    "name": "BIOS Password QR",
    "description": "Plugin for bios-pw.org to generate a scanable QR code on generated codes.",
    "version": "2.0",
    "author": "Josh Wallace",
    "content_scripts":[
        {
            "matches":["https://bios-pw.org/"],
            "js":["content.js"],
            "css":["override.css"]
        }
    ],
    "icons":{
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"
    }
}